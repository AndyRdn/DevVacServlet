<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1"
         metadata-complete="true">

    <servlet>
        <servlet-name>allEmployer</servlet-name>
        <servlet-class>servlet.ServletEmployer</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>allEmployer</servlet-name>
        <url-pattern>/allEmployer</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>insererEmployer</servlet-name>
        <servlet-class>servlet.InsererEmployerServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>insererEmployer</servlet-name>
        <url-pattern>/insererEmployer</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>allTable</servlet-name>
        <servlet-class>servlet.ServletTable</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>allTable</servlet-name>
        <url-pattern>/allTable</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>insererTable</servlet-name>
        <servlet-class>servlet.InsererTable</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>insererTable</servlet-name>
        <url-pattern>/insererTable</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>allSakafo</servlet-name>
        <servlet-class>servlet.ServletSakafo</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>allSakafo</servlet-name>
        <url-pattern>/allSakafo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>insererSakafo</servlet-name>
        <servlet-class>servlet.InsererSakafo</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>insererSakafo</servlet-name>
        <url-pattern>/insererSakafo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>allCommande</servlet-name>
        <servlet-class>servlet.ServletCommande</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>allCommande</servlet-name>
        <url-pattern>/allCommande</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>insererCommande</servlet-name>
        <servlet-class>servlet.InsererCommande</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>insererCommande</servlet-name>
        <url-pattern>/insererCommande</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.ServletLogin</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>


</web-app>
